            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:mps_vote-in_favour a gaf:Assertion,
            sem:Event ;
        rdfs:label "mps_vote-in_favour" ;
        gaf:denotedBy leolaniTalk:chat46_utterance5_char0-159 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

