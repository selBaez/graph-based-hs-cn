leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:hs a n2mu:hs,
            gaf:Instance ;
        rdfs:label "hs" ;
        gaf:denotedIn leolaniTalk:chat89_utterance3_char0-100 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:okay a gaf:Instance ;
        rdfs:label "okay" ;
        gaf:denotedIn leolaniTalk:chat89_utterance3_char0-100 .
}

leolaniWorld:Claims {
    leolaniWorld:hs_be_okay a gaf:Assertion,
            sem:Event ;
        rdfs:label "hs_be_okay" ;
        gaf:denotedBy leolaniTalk:chat89_utterance3_char0-100 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniWorld:hs_be_okay {
    leolaniWorld:hs n2mu:be leolaniWorld:okay .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat89_utterance3_char0-100 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat89_utterance3_char0-100" ;
        gaf:containsDenotation leolaniWorld:hs,
            leolaniWorld:okay ;
        gaf:denotes leolaniWorld:hs_be_okay ;
        grasp:hasAttribution leolaniTalk:hs_be_okay_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 20:15:52.704542"^^xml1:string ;
        rdf:value "I'm not okay with people being stripped of their rights, in the case of LGBT individuals or anybody."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat89_utterance3 .

    leolaniTalk:hs_be_okay_0000 a grasp:Attribution ;
        rdfs:label "hs_be_okay_0000" ;
        grasp:isAttributionFor leolaniTalk:chat89_utterance3_char0-100 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat89 .

    leolaniTalk:chat89 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat89" ;
        n2mu:id "89"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat89_utterance3 .

    leolaniTalk:chat89_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat89_utterance3" ;
        n2mu:id "3"^^xml1:string ;
