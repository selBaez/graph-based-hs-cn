            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:eu a n2mu:country,
            gaf:Instance ;
        rdfs:label "eu" ;
        gaf:denotedIn leolaniTalk:chat59_utterance3_char0-98 .

    leolaniWorld:law a n2mu:law,
            gaf:Instance ;
        rdfs:label "law" ;
        gaf:denotedIn leolaniTalk:chat59_utterance3_char0-98 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .
}

leolaniWorld:Claims {
    leolaniWorld:eu_have_law a gaf:Assertion,
            sem:Event ;
        rdfs:label "eu_have_law" ;
        gaf:denotedBy leolaniTalk:chat59_utterance3_char0-98 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat59_utterance3_char0-98 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat59_utterance3_char0-98" ;
        gaf:containsDenotation leolaniWorld:eu,
            leolaniWorld:law ;
        gaf:denotes leolaniWorld:eu_have_law ;
        grasp:hasAttribution leolaniTalk:eu_have_law_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 20:07:18.723414"^^xml1:string ;
        rdf:value "The EU and UK have laws against discrimination based on religion. Maybe you should check them out."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat59_utterance3 .

    leolaniTalk:eu_have_law_0000 a grasp:Attribution ;
        rdfs:label "eu_have_law_0000" ;
        grasp:isAttributionFor leolaniTalk:chat59_utterance3_char0-98 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
