leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat21 .

    leolaniTalk:chat21 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat21" ;
        n2mu:id "21"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat21_utterance3 .

    leolaniTalk:chat21_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat21_utterance3" ;
        n2mu:id "3"^^xml1:string ;
