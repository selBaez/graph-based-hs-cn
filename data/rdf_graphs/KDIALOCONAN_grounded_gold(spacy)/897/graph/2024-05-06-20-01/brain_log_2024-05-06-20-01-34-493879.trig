            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:movement_be_fight a gaf:Assertion,
            sem:Event ;
        rdfs:label "movement_be_fight" ;
        gaf:denotedBy leolaniTalk:chat21_utterance3_char0-76 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
