@prefix gaf: <http://groundedannotationframework.org/gaf#> .
@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix graspe: <http://groundedannotationframework.org/grasp/emotion#> .
@prefix graspf: <http://groundedannotationframework.org/grasp/factuality#> .
@prefix grasps: <http://groundedannotationframework.org/grasp/sentiment#> .
@prefix leolaniContext: <http://cltl.nl/leolani/context/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniWorld:Instances {
    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:muslims a n2mu:country,
            gaf:Instance ;
        rdfs:label "muslims" ;
        gaf:denotedIn leolaniTalk:chat121_utterance0_char0-50 .

    leolaniWorld:nothing a n2mu:drawing,
            gaf:Instance ;
        rdfs:label "nothing" ;
        gaf:denotedIn leolaniTalk:chat121_utterance0_char0-50 .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat121 .

    leolaniTalk:chat121 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat121" ;
        n2mu:id "121"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat121_utterance0 .

    leolaniTalk:chat121_utterance0 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat121_utterance0" ;
        n2mu:id "0"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:muslims_have_nothing a gaf:Assertion,
            sem:Event ;
        rdfs:label "muslims_have_nothing" ;
        gaf:denotedBy leolaniTalk:chat121_utterance0_char0-50 .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniWorld:muslims_have_nothing {
    leolaniWorld:muslims n2mu:have leolaniWorld:nothing .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat121_utterance0_char0-50 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat121_utterance0_char0-50" ;
        gaf:containsDenotation leolaniWorld:muslims,
            leolaniWorld:nothing ;
        gaf:denotes leolaniWorld:muslims_have_nothing ;
        grasp:hasAttribution leolaniTalk:muslims_have_nothing_0000 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-07 01:09:06.194808"^^xml1:string ;
        rdf:value "Muslims have nothing to contribute to our society."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat121_utterance0 .

    leolaniTalk:muslims_have_nothing_0000 a grasp:Attribution ;
        rdfs:label "muslims_have_nothing_0000" ;
        grasp:isAttributionFor leolaniTalk:chat121_utterance0_char0-50 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

