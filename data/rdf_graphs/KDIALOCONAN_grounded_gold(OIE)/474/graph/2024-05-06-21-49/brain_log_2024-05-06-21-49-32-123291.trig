        prov:wasDerivedFrom leolaniTalk:chat140_utterance0 .

    leolaniTalk:war_is_result_0000 a grasp:Attribution ;
        rdfs:label "war_is_result_0000" ;
        grasp:isAttributionFor leolaniTalk:chat140_utterance0_char0-88 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
