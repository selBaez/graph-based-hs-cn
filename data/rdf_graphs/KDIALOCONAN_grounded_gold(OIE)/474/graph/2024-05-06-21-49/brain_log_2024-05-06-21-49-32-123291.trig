            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:result a n2mu:result,
            gaf:Instance ;
        rdfs:label "result" ;
        gaf:denotedIn leolaniTalk:chat140_utterance0_char0-88 .

    leolaniWorld:war a n2mu:language,
            gaf:Instance ;
        rdfs:label "war" ;
        gaf:denotedIn leolaniTalk:chat140_utterance0_char0-88 .
}

leolaniTalk:Interactions {
