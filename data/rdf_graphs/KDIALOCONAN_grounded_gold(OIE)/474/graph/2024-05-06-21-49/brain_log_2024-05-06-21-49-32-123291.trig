leolaniWorld:war_is_result {
    leolaniWorld:war n2mu:is leolaniWorld:result .
}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
