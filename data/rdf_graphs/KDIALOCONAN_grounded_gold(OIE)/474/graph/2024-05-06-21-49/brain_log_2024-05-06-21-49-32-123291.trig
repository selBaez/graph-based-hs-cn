            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:war_is_result a gaf:Assertion,
            sem:Event ;
        rdfs:label "war_is_result" ;
        gaf:denotedBy leolaniTalk:chat140_utterance0_char0-88 .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat140_utterance0_char0-88 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat140_utterance0_char0-88" ;
        gaf:containsDenotation leolaniWorld:result,
            leolaniWorld:war ;
