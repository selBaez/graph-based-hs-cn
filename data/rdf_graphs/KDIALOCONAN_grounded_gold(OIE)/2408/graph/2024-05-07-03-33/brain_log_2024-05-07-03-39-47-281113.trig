leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:intolerant a gaf:Instance ;
        rdfs:label "intolerant" ;
        gaf:denotedIn leolaniTalk:chat205_utterance7_char0-92 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:muslims a n2mu:country,
            gaf:Instance ;
        rdfs:label "muslims" ;
        gaf:denotedIn leolaniTalk:chat205_utterance7_char0-92 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat205 .

    leolaniTalk:chat205 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat205" ;
        n2mu:id "205"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat205_utterance7 .

    leolaniTalk:chat205_utterance7 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat205_utterance7" ;
        n2mu:id "7"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:muslims_are_intolerant {
    leolaniWorld:muslims n2mu:are leolaniWorld:intolerant .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:muslims_are_intolerant a gaf:Assertion,
            sem:Event ;
        rdfs:label "muslims_are_intolerant" ;
        gaf:denotedBy leolaniTalk:chat205_utterance7_char0-92 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat205_utterance7_char0-92 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat205_utterance7_char0-92" ;
        gaf:containsDenotation leolaniWorld:intolerant,
            leolaniWorld:muslims ;
        gaf:denotes leolaniWorld:muslims_are_intolerant ;
        grasp:hasAttribution leolaniTalk:muslims_are_intolerant_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 03:39:40.370510"^^xml1:string ;
        rdf:value "Are you suggesting that all Muslims are intolerant? I don't understand how that could happen"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat205_utterance7 .

    leolaniTalk:muslims_are_intolerant_0000 a grasp:Attribution ;
        rdfs:label "muslims_are_intolerant_0000" ;
        grasp:isAttributionFor leolaniTalk:chat205_utterance7_char0-92 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
