        rdfs:label "benefit" ;
        gaf:denotedIn leolaniTalk:chat52_utterance2_char0-90 .

    leolaniWorld:hs a n2mu:agent,
            n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" ;
        gaf:denotedIn leolaniTalk:chat52_utterance2_char0-90 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .
}

leolaniWorld:hs_experienced_benefit {
    leolaniWorld:hs n2mu:experienced leolaniWorld:benefit .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat52 .

    leolaniTalk:chat52 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat52" ;
        n2mu:id "52"^^xml1:string ;
