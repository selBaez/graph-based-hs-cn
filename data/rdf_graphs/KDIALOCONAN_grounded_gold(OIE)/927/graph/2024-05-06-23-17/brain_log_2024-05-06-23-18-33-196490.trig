    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:hs_experienced_benefit a gaf:Assertion,
            sem:Event ;
        rdfs:label "hs_experienced_benefit" ;
        gaf:denotedBy leolaniTalk:chat52_utterance2_char0-90 .

    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat52_utterance2_char0-90 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat52_utterance2_char0-90" ;
        gaf:containsDenotation leolaniWorld:benefit,
