    leolaniTalk:hs_experienced_benefit_0000 a grasp:Attribution ;
        rdfs:label "hs_experienced_benefit_0000" ;
        grasp:isAttributionFor leolaniTalk:chat52_utterance2_char0-90 ;
        rdf:value graspe:UNDERSPECIFIED,
