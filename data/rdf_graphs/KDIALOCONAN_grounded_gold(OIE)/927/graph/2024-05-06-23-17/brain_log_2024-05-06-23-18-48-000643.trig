    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:muslims_have_nothing a gaf:Assertion,
            sem:Event ;
        rdfs:label "muslims_have_nothing" ;
        gaf:denotedBy leolaniTalk:chat52_utterance3_char0-104 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniWorld:muslims_have_nothing {
    leolaniWorld:muslims n2mu:have leolaniWorld:nothing .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat52_utterance3_char0-104 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat52_utterance3_char0-104" ;
        gaf:containsDenotation leolaniWorld:muslims,
            leolaniWorld:nothing ;
        gaf:denotes leolaniWorld:muslims_have_nothing ;
        grasp:hasAttribution leolaniTalk:muslims_have_nothing_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 23:18:41.173014"^^xml1:string ;
        rdf:value "Are you sure that Muslims have nothing to contribute to our society? Do you have any statistics on that?"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat52_utterance3 .

    leolaniTalk:muslims_have_nothing_0000 a grasp:Attribution ;
        rdfs:label "muslims_have_nothing_0000" ;
        grasp:isAttributionFor leolaniTalk:chat52_utterance3_char0-104 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
