leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:muslims a n2mu:country,
            gaf:Instance ;
        rdfs:label "muslims" ;
        gaf:denotedIn leolaniTalk:chat52_utterance3_char0-104 .

    leolaniWorld:nothing a gaf:Instance ;
        rdfs:label "nothing" ;
        gaf:denotedIn leolaniTalk:chat52_utterance3_char0-104 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat52 .

    leolaniTalk:chat52 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat52" ;
        n2mu:id "52"^^xml1:string ;
