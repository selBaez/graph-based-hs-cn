            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:is_problem a gaf:Instance ;
        rdfs:label "is-problem" ;
        gaf:denotedIn leolaniTalk:chat47_utterance5_char0-155 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:people a n2mu:deutschland,
            gaf:Instance ;
        rdfs:label "people" ;
        gaf:denotedIn leolaniTalk:chat47_utterance5_char0-155 .
}

leolaniWorld:leolani_know_cn {
