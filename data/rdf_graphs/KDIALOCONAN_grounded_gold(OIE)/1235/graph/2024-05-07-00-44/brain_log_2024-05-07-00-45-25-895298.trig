    leolaniTalk:people_will-find_is_problem_0000 a grasp:Attribution ;
        rdfs:label "people_will-find_is_problem_0000" ;
        grasp:isAttributionFor leolaniTalk:chat47_utterance5_char0-155 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
