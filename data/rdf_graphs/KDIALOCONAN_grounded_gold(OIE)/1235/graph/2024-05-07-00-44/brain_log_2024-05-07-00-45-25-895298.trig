    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat47_utterance5_char0-155 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat47_utterance5_char0-155" ;
        gaf:containsDenotation leolaniWorld:is_problem,
            leolaniWorld:people ;
        gaf:denotes leolaniWorld:people_will-find_is_problem ;
        grasp:hasAttribution leolaniTalk:people_will-find_is_problem_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 00:45:17.582161"^^xml1:string ;
        rdf:value "They are, and if there is a problem, people will find a solution through dialogue. Instead of dividing people, we should be working on our common humanity."^^xml1:string ;
