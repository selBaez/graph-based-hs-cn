            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:prank_planned-by_non-muslims_4chan a gaf:Assertion,
            sem:Event ;
        rdfs:label "prank_planned-by_non-muslims_4chan" ;
        gaf:denotedBy leolaniTalk:chat204_utterance1_char0-152 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat204_utterance1_char0-152 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat204_utterance1_char0-152" ;
