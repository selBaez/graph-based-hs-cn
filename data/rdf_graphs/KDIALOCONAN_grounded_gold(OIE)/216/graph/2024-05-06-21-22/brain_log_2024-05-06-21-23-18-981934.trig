            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:non-muslims_4chan a gaf:Instance ;
        rdfs:label "non-muslims-4chan" ;
        gaf:denotedIn leolaniTalk:chat204_utterance1_char0-152 .

    leolaniWorld:prank a n2mu:record-label,
            gaf:Instance ;
        rdfs:label "prank" ;
        gaf:denotedIn leolaniTalk:chat204_utterance1_char0-152 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat204 .

    leolaniTalk:chat204 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat204" ;
        n2mu:id "204"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat204_utterance1 .

    leolaniTalk:chat204_utterance1 a grasp:Utterance,
