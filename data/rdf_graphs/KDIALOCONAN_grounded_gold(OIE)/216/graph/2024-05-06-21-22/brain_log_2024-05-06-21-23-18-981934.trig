        prov:wasDerivedFrom leolaniTalk:chat204_utterance1 .

    leolaniTalk:prank_planned-by_non-muslims_4chan_0000 a grasp:Attribution ;
        rdfs:label "prank_planned-by_non-muslims_4chan_0000" ;
        grasp:isAttributionFor leolaniTalk:chat204_utterance1_char0-152 ;
        rdf:value graspe:UNDERSPECIFIED,
