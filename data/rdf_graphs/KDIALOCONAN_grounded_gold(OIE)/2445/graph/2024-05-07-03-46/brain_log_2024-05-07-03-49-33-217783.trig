    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:blood_should_should_donated a gaf:Assertion,
            sem:Event ;
        rdfs:label "blood_should_should_donated" ;
        gaf:denotedBy leolaniTalk:chat192_utterance3_char0-71 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniWorld:blood_should_should_donated {
    leolaniWorld:blood n2mu:should leolaniWorld:should_donated .
}

leolaniTalk:Perspectives {
    leolaniTalk:blood_should_should_donated_0000 a grasp:Attribution ;
        rdfs:label "blood_should_should_donated_0000" ;
        grasp:isAttributionFor leolaniTalk:chat192_utterance3_char0-71 ;
        rdf:value graspe:UNDERSPECIFIED,
