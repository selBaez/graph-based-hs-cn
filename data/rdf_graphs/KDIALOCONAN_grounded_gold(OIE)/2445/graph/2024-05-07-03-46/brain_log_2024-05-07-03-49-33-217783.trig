            grasps:UNDERSPECIFIED .

    leolaniTalk:chat192_utterance3_char0-71 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat192_utterance3_char0-71" ;
        gaf:containsDenotation leolaniWorld:blood,
            leolaniWorld:should_donated ;
        gaf:denotes leolaniWorld:blood_should_should_donated ;
        grasp:hasAttribution leolaniTalk:blood_should_should_donated_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 03:49:22.222958"^^xml1:string ;
        rdf:value "Blood should be donated if also stem cell donations is allowed by MSMs."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat192_utterance3 .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
