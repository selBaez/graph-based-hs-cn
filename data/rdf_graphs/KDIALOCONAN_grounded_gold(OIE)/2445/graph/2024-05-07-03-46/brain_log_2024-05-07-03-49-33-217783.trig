leolaniWorld:Instances {
    leolaniWorld:blood a n2mu:anatomical-structure,
            gaf:Instance ;
        rdfs:label "blood" ;
        gaf:denotedIn leolaniTalk:chat192_utterance3_char0-71 .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:should_donated a gaf:Instance ;
        rdfs:label "should-donated" ;
        gaf:denotedIn leolaniTalk:chat192_utterance3_char0-71 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat192 .

    leolaniTalk:chat192 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat192" ;
        n2mu:id "192"^^xml1:string ;
