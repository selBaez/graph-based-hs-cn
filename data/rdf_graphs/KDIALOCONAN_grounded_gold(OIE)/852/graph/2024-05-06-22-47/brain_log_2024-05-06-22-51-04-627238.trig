leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:equate a gaf:Instance ;
        rdfs:label "equate" ;
        gaf:denotedIn leolaniTalk:chat57_utterance5_char0-150 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:propaganda a n2mu:music-genre,
            gaf:Instance ;
        rdfs:label "propaganda" ;
        gaf:denotedIn leolaniTalk:chat57_utterance5_char0-150 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat57 .

    leolaniTalk:chat57 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat57" ;
        n2mu:id "57"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat57_utterance5 .

    leolaniTalk:chat57_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat57_utterance5" ;
        n2mu:id "5"^^xml1:string ;
