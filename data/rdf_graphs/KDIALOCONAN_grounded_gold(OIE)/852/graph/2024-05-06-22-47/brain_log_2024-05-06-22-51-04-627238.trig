    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:equate_is_propaganda {
    leolaniWorld:equate n2mu:is leolaniWorld:propaganda .
}

leolaniWorld:Claims {
    leolaniWorld:equate_is_propaganda a gaf:Assertion,
            sem:Event ;
        rdfs:label "equate_is_propaganda" ;
        gaf:denotedBy leolaniTalk:chat57_utterance5_char0-150 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat57_utterance5_char0-150 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat57_utterance5_char0-150" ;
        gaf:containsDenotation leolaniWorld:equate,
            leolaniWorld:propaganda ;
        gaf:denotes leolaniWorld:equate_is_propaganda ;
        grasp:hasAttribution leolaniTalk:equate_is_propaganda_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 22:50:49.095213"^^xml1:string ;
        rdf:value "This is a myth which was created by anti-gay activists to demonise LGBTI people. To equate LGBTI people with paedophiles is just dangerous propaganda."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat57_utterance5 .

    leolaniTalk:equate_is_propaganda_0000 a grasp:Attribution ;
        rdfs:label "equate_is_propaganda_0000" ;
        grasp:isAttributionFor leolaniTalk:chat57_utterance5_char0-150 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
