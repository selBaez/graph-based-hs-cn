leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:islam a n2mu:ethnic-group,
            gaf:Instance ;
        rdfs:label "islam" ;
        gaf:denotedIn leolaniTalk:chat163_utterance3_char0-99 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:peaceful a gaf:Instance ;
        rdfs:label "peaceful" ;
        gaf:denotedIn leolaniTalk:chat163_utterance3_char0-99 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat163 .

    leolaniTalk:chat163 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat163" ;
        n2mu:id "163"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat163_utterance3 .

    leolaniTalk:chat163_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat163_utterance3" ;
        n2mu:id "3"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:islam_is_peaceful {
    leolaniWorld:islam n2mu:is leolaniWorld:peaceful .
}

leolaniWorld:Claims {
    leolaniWorld:islam_is_peaceful a gaf:Assertion,
            sem:Event ;
        rdfs:label "islam_is_peaceful" ;
        gaf:denotedBy leolaniTalk:chat163_utterance3_char0-99 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat163_utterance3_char0-99 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat163_utterance3_char0-99" ;
        gaf:containsDenotation leolaniWorld:islam,
            leolaniWorld:peaceful ;
        gaf:denotes leolaniWorld:islam_is_peaceful ;
        grasp:hasAttribution leolaniTalk:islam_is_peaceful_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 03:02:05.037720"^^xml1:string ;
        rdf:value "Islam is a peaceful religion and its followers are already integrated into many European societies."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat163_utterance3 .

    leolaniTalk:islam_is_peaceful_0000 a grasp:Attribution ;
        rdfs:label "islam_is_peaceful_0000" ;
        grasp:isAttributionFor leolaniTalk:chat163_utterance3_char0-99 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

