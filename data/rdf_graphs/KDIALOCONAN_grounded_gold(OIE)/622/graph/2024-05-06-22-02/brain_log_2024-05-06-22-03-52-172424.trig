    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniWorld:one_group_trying_to_understand {
    leolaniWorld:one_group n2mu:trying leolaniWorld:to_understand .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat110_utterance1_char0-159 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat110_utterance1_char0-159" ;
