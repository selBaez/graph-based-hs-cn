}

leolaniTalk:Perspectives {
    leolaniTalk:chat253_utterance1_char0-152 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat253_utterance1_char0-152" ;
        gaf:containsDenotation leolaniWorld:more_tolerant,
