            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:none_were_more_tolerant a gaf:Assertion,
            sem:Event ;
        rdfs:label "none_were_more_tolerant" ;
        gaf:denotedBy leolaniTalk:chat253_utterance1_char0-152 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniWorld:none_were_more_tolerant {
