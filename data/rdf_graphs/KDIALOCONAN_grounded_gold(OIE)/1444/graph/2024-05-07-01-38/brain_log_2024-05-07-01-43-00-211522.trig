            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:more_tolerant a gaf:Instance ;
        rdfs:label "more-tolerant" ;
