    leolaniTalk:none_were_more_tolerant_0000 a grasp:Attribution ;
        rdfs:label "none_were_more_tolerant_0000" ;
        grasp:isAttributionFor leolaniTalk:chat253_utterance1_char0-152 ;
        rdf:value graspe:UNDERSPECIFIED,
