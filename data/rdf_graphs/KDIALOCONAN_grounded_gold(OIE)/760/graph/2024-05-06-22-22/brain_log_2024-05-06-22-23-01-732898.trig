leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:hs a n2mu:hs,
            gaf:Instance ;
        rdfs:label "hs" ;
        gaf:denotedIn leolaniTalk:chat221_utterance1_char0-147 .

    leolaniWorld:it a n2mu:it,
            gaf:Instance ;
        rdfs:label "it" ;
        gaf:denotedIn leolaniTalk:chat221_utterance1_char0-147 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat221 .

    leolaniTalk:chat221 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat221" ;
        n2mu:id "221"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat221_utterance1 .

    leolaniTalk:chat221_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat221_utterance1" ;
        n2mu:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:it_seems-to_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "it_seems-to_hs" ;
        gaf:denotedBy leolaniTalk:chat221_utterance1_char0-147 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat221_utterance1_char0-147 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat221_utterance1_char0-147" ;
        gaf:containsDenotation leolaniWorld:hs,
            leolaniWorld:it ;
        gaf:denotes leolaniWorld:it_seems-to_hs ;
        grasp:hasAttribution leolaniTalk:it_seems-to_hs_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 22:22:57.656877"^^xml1:string ;
        rdf:value "Just because you dont like gays doesn't mean you have to hate them. What did they do to you? It seems to me that you still have all of your rights."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat221_utterance1 .

    leolaniTalk:it_seems-to_hs_0000 a grasp:Attribution ;
        rdfs:label "it_seems-to_hs_0000" ;
        grasp:isAttributionFor leolaniTalk:chat221_utterance1_char0-147 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:it_seems-to_hs {
    leolaniWorld:it n2mu:seems_to leolaniWorld:hs .
}

