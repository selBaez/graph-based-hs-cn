leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:general a n2mu:general,
            gaf:Instance ;
        rdfs:label "general" ;
        gaf:denotedIn leolaniTalk:chat195_utterance1_char0-160 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:other_people a n2mu:artifact,
            n2mu:object,
            gaf:Instance ;
        rdfs:label "other-people" ;
        gaf:denotedIn leolaniTalk:chat195_utterance1_char0-160 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat195 .

    leolaniTalk:chat195 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat195" ;
        n2mu:id "195"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat195_utterance1 .

    leolaniTalk:chat195_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat195_utterance1" ;
        n2mu:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:other_people_is-in_general a gaf:Assertion,
            sem:Event ;
        rdfs:label "other_people_is-in_general" ;
        gaf:denotedBy leolaniTalk:chat195_utterance1_char0-160 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniWorld:other_people_is-in_general {
    leolaniWorld:other_people n2mu:is_in leolaniWorld:general .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat195_utterance1_char0-160 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat195_utterance1_char0-160" ;
        gaf:containsDenotation leolaniWorld:general,
            leolaniWorld:other_people ;
        gaf:denotes leolaniWorld:other_people_is-in_general ;
        grasp:hasAttribution leolaniTalk:other_people_is-in_general_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 21:34:39.472337"^^xml1:string ;
        rdf:value "Muslims do vote like other people, in general in favor of the people they perceive as closer to their culture and identity. That's why it is called \"democracy\"."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat195_utterance1 .

    leolaniTalk:other_people_is-in_general_0000 a grasp:Attribution ;
        rdfs:label "other_people_is-in_general_0000" ;
        grasp:isAttributionFor leolaniTalk:chat195_utterance1_char0-160 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

