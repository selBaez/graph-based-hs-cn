leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:muslims a n2mu:country,
            gaf:Instance ;
        rdfs:label "muslims" ;
        gaf:denotedIn leolaniTalk:chat204_utterance3_char0-142 .

    leolaniWorld:our_country a gaf:Instance ;
        rdfs:label "our-country" ;
        gaf:denotedIn leolaniTalk:chat204_utterance3_char0-142 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat204 .

    leolaniTalk:chat204 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat204" ;
        n2mu:id "204"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat204_utterance3 .

    leolaniTalk:chat204_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat204_utterance3" ;
        n2mu:id "3"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:muslims_have-contributed-to_our_country a gaf:Assertion,
            sem:Event ;
        rdfs:label "muslims_have-contributed-to_our_country" ;
        gaf:denotedBy leolaniTalk:chat204_utterance3_char0-142 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniWorld:muslims_have-contributed-to_our_country {
    leolaniWorld:muslims n2mu:have_contributed_to leolaniWorld:our_country .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat204_utterance3_char0-142 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat204_utterance3_char0-142" ;
        gaf:containsDenotation leolaniWorld:muslims,
            leolaniWorld:our_country ;
        gaf:denotes leolaniWorld:muslims_have-contributed-to_our_country ;
        grasp:hasAttribution leolaniTalk:muslims_have-contributed-to_our_country_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 00:58:36.387476"^^xml1:string ;
        rdf:value "Muslims have contributed to our country, and they are part of the fabric of our society. To suggest they are a 'chronic threat' is just wrong."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat204_utterance3 .

    leolaniTalk:muslims_have-contributed-to_our_country_0000 a grasp:Attribution ;
        rdfs:label "muslims_have-contributed-to_our_country_0000" ;
        grasp:isAttributionFor leolaniTalk:chat204_utterance3_char0-142 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

