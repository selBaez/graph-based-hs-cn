leolaniWorld:Instances {
    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:special a n2mu:special,
            gaf:Instance ;
        rdfs:label "special" ;
        gaf:denotedIn leolaniTalk:chat33_utterance0_char0-30 .

    leolaniWorld:they a gaf:Instance ;
        rdfs:label "they" ;
        gaf:denotedIn leolaniTalk:chat33_utterance0_char0-30 .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat33 .

    leolaniTalk:chat33 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat33" ;
        n2mu:id "33"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat33_utterance0 .

    leolaniTalk:chat33_utterance0 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat33_utterance0" ;
        n2mu:id "0"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:they_are_special a gaf:Assertion,
            sem:Event ;
        rdfs:label "they_are_special" ;
        gaf:denotedBy leolaniTalk:chat33_utterance0_char0-30 .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat33_utterance0_char0-30 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat33_utterance0_char0-30" ;
        gaf:containsDenotation leolaniWorld:special,
            leolaniWorld:they ;
        gaf:denotes leolaniWorld:they_are_special ;
        grasp:hasAttribution leolaniTalk:they_are_special_0000 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-07 03:50:48.683171"^^xml1:string ;
        rdf:value "Jews think they are so special"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat33_utterance0 .

    leolaniTalk:they_are_special_0000 a grasp:Attribution ;
        rdfs:label "they_are_special_0000" ;
        grasp:isAttributionFor leolaniTalk:chat33_utterance0_char0-30 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:they_are_special {
    leolaniWorld:they n2mu:are leolaniWorld:special .
}

