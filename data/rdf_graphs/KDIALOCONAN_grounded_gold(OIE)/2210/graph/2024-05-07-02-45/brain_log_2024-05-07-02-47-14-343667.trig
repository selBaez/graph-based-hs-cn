leolaniWorld:Instances {
    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:jews a n2mu:nation,
            gaf:Instance ;
        rdfs:label "jews" ;
        gaf:denotedIn leolaniTalk:chat89_utterance2_char0-62 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:secret_plot a gaf:Instance ;
        rdfs:label "secret-plot" ;
        gaf:denotedIn leolaniTalk:chat89_utterance2_char0-62 .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat89 .

    leolaniTalk:chat89 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat89" ;
        n2mu:id "89"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat89_utterance2 .

    leolaniTalk:chat89_utterance2 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat89_utterance2" ;
        n2mu:id "2"^^xml1:string ;
