    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

leolaniWorld:jews_have_secret_plot {
    leolaniWorld:jews n2mu:have leolaniWorld:secret_plot .
}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:jews_have_secret_plot a gaf:Assertion,
            sem:Event ;
        rdfs:label "jews_have_secret_plot" ;
        gaf:denotedBy leolaniTalk:chat89_utterance2_char0-62 .

    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat89_utterance2_char0-62 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat89_utterance2_char0-62" ;
        gaf:containsDenotation leolaniWorld:jews,
            leolaniWorld:secret_plot ;
        gaf:denotes leolaniWorld:jews_have_secret_plot ;
        grasp:hasAttribution leolaniTalk:jews_have_secret_plot_0000 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-07 02:47:07.466337"^^xml1:string ;
        rdf:value "Not a threat?! Jews have a secret plot to take over the world!"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat89_utterance2 .

    leolaniTalk:jews_have_secret_plot_0000 a grasp:Attribution ;
        rdfs:label "jews_have_secret_plot_0000" ;
        grasp:isAttributionFor leolaniTalk:chat89_utterance2_char0-62 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
