leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:job a n2mu:job,
            gaf:Instance ;
        rdfs:label "job" ;
        gaf:denotedIn leolaniTalk:chat70_utterance5_char0-170 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:muslims a n2mu:country,
            gaf:Instance ;
        rdfs:label "muslims" ;
        gaf:denotedIn leolaniTalk:chat70_utterance5_char0-170 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat70 .

    leolaniTalk:chat70 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat70" ;
        n2mu:id "70"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat70_utterance5 .

    leolaniTalk:chat70_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat70_utterance5" ;
        n2mu:id "5"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:muslims_find_job {
    leolaniWorld:muslims n2mu:find leolaniWorld:job .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:muslims_find_job a gaf:Assertion,
            sem:Event ;
        rdfs:label "muslims_find_job" ;
        gaf:denotedBy leolaniTalk:chat70_utterance5_char0-170 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat70_utterance5_char0-170 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat70_utterance5_char0-170" ;
        gaf:containsDenotation leolaniWorld:job,
            leolaniWorld:muslims ;
        gaf:denotes leolaniWorld:muslims_find_job ;
        grasp:hasAttribution leolaniTalk:muslims_find_job_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 03:46:18.948456"^^xml1:string ;
        rdf:value "You know why Muslims struggle to find a job? Job applicants with non-Muslim names are three times more likely to be interviewed for the job than people with Muslim names."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat70_utterance5 .

    leolaniTalk:muslims_find_job_0000 a grasp:Attribution ;
        rdfs:label "muslims_find_job_0000" ;
        grasp:isAttributionFor leolaniTalk:chat70_utterance5_char0-170 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

