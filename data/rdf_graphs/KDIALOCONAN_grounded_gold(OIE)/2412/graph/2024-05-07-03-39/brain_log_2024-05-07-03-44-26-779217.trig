leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:that a n2mu:food,
            gaf:Instance ;
        rdfs:label "that" ;
        gaf:denotedIn leolaniTalk:chat70_utterance3_char0-72 .

    leolaniWorld:wrong a gaf:Instance ;
        rdfs:label "wrong" ;
        gaf:denotedIn leolaniTalk:chat70_utterance3_char0-72 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat70 .

    leolaniTalk:chat70 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat70" ;
        n2mu:id "70"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat70_utterance3 .

    leolaniTalk:chat70_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat70_utterance3" ;
        n2mu:id "3"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:that_has_wrong a gaf:Assertion,
            sem:Event ;
        rdfs:label "that_has_wrong" ;
        gaf:denotedBy leolaniTalk:chat70_utterance3_char0-72 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat70_utterance3_char0-72 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat70_utterance3_char0-72" ;
        gaf:containsDenotation leolaniWorld:that,
            leolaniWorld:wrong ;
        gaf:denotes leolaniWorld:that_has_wrong ;
        grasp:hasAttribution leolaniTalk:that_has_wrong_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 03:44:15.914578"^^xml1:string ;
        rdf:value "That's wrong. Why would Muslims be incompatible with European societies?"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat70_utterance3 .

    leolaniTalk:that_has_wrong_0000 a grasp:Attribution ;
        rdfs:label "that_has_wrong_0000" ;
        grasp:isAttributionFor leolaniTalk:chat70_utterance3_char0-72 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:that_has_wrong {
    leolaniWorld:that n2mu:has leolaniWorld:wrong .
}

