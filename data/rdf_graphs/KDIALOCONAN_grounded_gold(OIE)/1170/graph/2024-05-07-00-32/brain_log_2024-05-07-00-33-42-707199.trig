    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat233_utterance3_char0-98 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat233_utterance3_char0-98" ;
