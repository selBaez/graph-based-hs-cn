    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat128_utterance5_char0-181 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat128_utterance5_char0-181" ;
        gaf:containsDenotation leolaniWorld:hs,
