            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:hs a n2mu:agent,
            gaf:Instance ;
        rdfs:label "hs" ;
        gaf:denotedIn leolaniTalk:chat128_utterance5_char0-181 .

    leolaniWorld:jews a n2mu:interest,
            gaf:Instance ;
        rdfs:label "jews" ;
        gaf:denotedIn leolaniTalk:chat128_utterance5_char0-181 .

    leolaniWorld:leolani a n2mu:robot ;
