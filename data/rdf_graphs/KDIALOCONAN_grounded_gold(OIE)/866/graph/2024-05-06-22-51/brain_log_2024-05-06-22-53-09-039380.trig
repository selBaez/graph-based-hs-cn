leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:country a n2mu:country,
            gaf:Instance ;
        rdfs:label "country" ;
        gaf:denotedIn leolaniTalk:chat9_utterance1_char0-121 .

    leolaniWorld:integration a gaf:Instance ;
        rdfs:label "integration" ;
        gaf:denotedIn leolaniTalk:chat9_utterance1_char0-121 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat9 .

    leolaniTalk:chat9 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat9" ;
        n2mu:id "9"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat9_utterance1 .

    leolaniTalk:chat9_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat9_utterance1" ;
        n2mu:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:country_takes_integration a gaf:Assertion,
            sem:Event ;
        rdfs:label "country_takes_integration" ;
        gaf:denotedBy leolaniTalk:chat9_utterance1_char0-121 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat9_utterance1_char0-121 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat9_utterance1_char0-121" ;
        gaf:containsDenotation leolaniWorld:country,
            leolaniWorld:integration ;
        gaf:denotes leolaniWorld:country_takes_integration ;
        grasp:hasAttribution leolaniTalk:country_takes_integration_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 22:53:02.187050"^^xml1:string ;
        rdf:value "The more a country takes integration as national policy, the more the citizens accept each other: see for example [link]."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat9_utterance1 .

    leolaniTalk:country_takes_integration_0000 a grasp:Attribution ;
        rdfs:label "country_takes_integration_0000" ;
        grasp:isAttributionFor leolaniTalk:chat9_utterance1_char0-121 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:country_takes_integration {
    leolaniWorld:country n2mu:takes leolaniWorld:integration .
}

