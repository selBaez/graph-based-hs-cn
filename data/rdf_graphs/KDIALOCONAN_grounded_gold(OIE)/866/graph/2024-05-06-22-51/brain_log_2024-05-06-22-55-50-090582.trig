leolaniWorld:Claims {
    leolaniWorld:hs_should-focus-on_rights a gaf:Assertion,
            sem:Event ;
        rdfs:label "hs_should-focus-on_rights" ;
        gaf:denotedBy leolaniTalk:chat9_utterance7_char0-256 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat9_utterance7_char0-256 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat9_utterance7_char0-256" ;
        gaf:containsDenotation leolaniWorld:hs,
            leolaniWorld:rights ;
        gaf:denotes leolaniWorld:hs_should-focus-on_rights ;
        grasp:hasAttribution leolaniTalk:hs_should-focus-on_rights_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 22:55:45.976047"^^xml1:string ;
        rdf:value "It is important to point out that only a small part of the world's population has migrated. We should focus on guaranteeing equal rights to the immigrants that are already in the country, rather than blocking out those who are trying to live a better life."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat9_utterance7 .

    leolaniTalk:hs_should-focus-on_rights_0000 a grasp:Attribution ;
        rdfs:label "hs_should-focus-on_rights_0000" ;
        grasp:isAttributionFor leolaniTalk:chat9_utterance7_char0-256 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:hs_should-focus-on_rights {
