            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:hs a n2mu:hs,
            gaf:Instance ;
        rdfs:label "hs" ;
        gaf:denotedIn leolaniTalk:chat9_utterance7_char0-256 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:rights a n2mu:rights,
            gaf:Instance ;
        rdfs:label "rights" ;
        gaf:denotedIn leolaniTalk:chat9_utterance7_char0-256 .
}

leolaniWorld:leolani_know_cn {
