            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:nothing a gaf:Instance ;
        rdfs:label "nothing" ;
        gaf:denotedIn leolaniTalk:chat239_utterance5_char0-339 .

    leolaniWorld:problem a n2mu:problem,
            gaf:Instance ;
        rdfs:label "problem" ;
        gaf:denotedIn leolaniTalk:chat239_utterance5_char0-339 .
}

leolaniWorld:leolani_know_cn {
