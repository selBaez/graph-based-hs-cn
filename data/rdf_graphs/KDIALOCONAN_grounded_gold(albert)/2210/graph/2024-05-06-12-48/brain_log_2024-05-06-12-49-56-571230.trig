leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:someone_should-be_respected a gaf:Assertion,
            sem:Event ;
        rdfs:label "someone_should-be_respected" ;
        gaf:denotedBy leolaniTalk:chat234_utterance2_char0-62 .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniWorld:Instances {
    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:respected a gaf:Instance ;
        rdfs:label "respected" ;
        gaf:denotedIn leolaniTalk:chat234_utterance2_char0-62 .

    leolaniWorld:someone a gaf:Instance ;
        rdfs:label "someone" ;
        gaf:denotedIn leolaniTalk:chat234_utterance2_char0-62 .
}

leolaniWorld:someone_should-be_respected {
    leolaniWorld:someone n2mu:should-be leolaniWorld:respected .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat234_utterance2_char0-62 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat234_utterance2_char0-62" ;
        gaf:containsDenotation leolaniWorld:respected,
            leolaniWorld:someone ;
        gaf:denotes leolaniWorld:someone_should-be_respected ;
        grasp:hasAttribution leolaniTalk:someone_should-be_respected_2120 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-06 12:49:42.719975"^^xml1:string ;
        rdf:value "Not a threat?! Jews have a secret plot to take over the world!"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat234_utterance2 .

    leolaniTalk:someone_should-be_respected_2120 a grasp:Attribution ;
        rdfs:label "someone_should-be_respected_2120" ;
        grasp:isAttributionFor leolaniTalk:chat234_utterance2_char0-62 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:POSITIVE,
            graspf:PROBABLE,
            grasps:NEUTRAL .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:POSITIVE a grasp:AttributionValue,
            graspf:PolarityValue ;
        rdfs:label "POSITIVE" .

    graspf:PROBABLE a grasp:AttributionValue,
            graspf:CertaintyValue ;
        rdfs:label "PROBABLE" .

    grasps:NEUTRAL a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "NEUTRAL" .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat234 .

    leolaniTalk:chat234 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat234" ;
        n2mu:id "234"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat234_utterance2 .

    leolaniTalk:chat234_utterance2 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat234_utterance2" ;
        n2mu:id "2"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
