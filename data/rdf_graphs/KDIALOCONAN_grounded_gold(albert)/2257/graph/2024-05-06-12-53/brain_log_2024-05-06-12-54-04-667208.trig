leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:muslims_stay_here a gaf:Assertion,
            sem:Event ;
        rdfs:label "muslims_stay_here" ;
        gaf:denotedBy leolaniTalk:chat32_utterance2_char0-58 .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniWorld:Instances {
    leolaniWorld:here a gaf:Instance ;
        rdfs:label "here" ;
        gaf:denotedIn leolaniTalk:chat32_utterance2_char0-58 .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:muslims a n2mu:country,
            gaf:Instance ;
        rdfs:label "muslims" ;
        gaf:denotedIn leolaniTalk:chat32_utterance2_char0-58 .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat32_utterance2_char0-58 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat32_utterance2_char0-58" ;
        gaf:containsDenotation leolaniWorld:here,
            leolaniWorld:muslims ;
        gaf:denotes leolaniWorld:muslims_stay_here ;
        grasp:hasAttribution leolaniTalk:muslims_stay_here_3-120 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-06 12:53:52.751843"^^xml1:string ;
        rdf:value "We should get rid of all of them, Muslims can't stay here."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat32_utterance2 .

    leolaniTalk:muslims_stay_here_3-120 a grasp:Attribution ;
        rdfs:label "muslims_stay_here_3-120" ;
        grasp:isAttributionFor leolaniTalk:chat32_utterance2_char0-58 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:CERTAIN,
            graspf:NEGATIVE,
            grasps:NEUTRAL .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:CERTAIN a grasp:AttributionValue,
            graspf:CertaintyValue ;
        rdfs:label "CERTAIN" .

    graspf:NEGATIVE a grasp:AttributionValue,
            graspf:PolarityValue ;
        rdfs:label "NEGATIVE" .

    grasps:NEUTRAL a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "NEUTRAL" .
}

leolaniWorld:muslims_stay_here {
    leolaniWorld:muslims n2mu:stay leolaniWorld:here .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat32 .

    leolaniTalk:chat32 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat32" ;
        n2mu:id "32"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat32_utterance2 .

    leolaniTalk:chat32_utterance2 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat32_utterance2" ;
        n2mu:id "2"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

