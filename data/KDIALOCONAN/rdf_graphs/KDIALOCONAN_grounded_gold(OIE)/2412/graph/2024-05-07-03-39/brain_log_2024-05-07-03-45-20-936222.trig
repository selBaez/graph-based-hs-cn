@prefix gaf: <http://groundedannotationframework.org/gaf#> .
@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix graspe: <http://groundedannotationframework.org/grasp/emotion#> .
@prefix graspf: <http://groundedannotationframework.org/grasp/factuality#> .
@prefix grasps: <http://groundedannotationframework.org/grasp/sentiment#> .
@prefix leolaniContext: <http://cltl.nl/leolani/context/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniWorld:Instances {
    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:majority a n2mu:majority,
            gaf:Instance ;
        rdfs:label "majority" ;
        gaf:denotedIn leolaniTalk:chat70_utterance4_char0-71 .

    leolaniWorld:work a n2mu:work,
            gaf:Instance ;
        rdfs:label "work" ;
        gaf:denotedIn leolaniTalk:chat70_utterance4_char0-71 .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat70 .

    leolaniTalk:chat70 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat70" ;
        n2mu:id "70"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat70_utterance4 .

    leolaniTalk:chat70_utterance4 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat70_utterance4" ;
        n2mu:id "4"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

leolaniWorld:majority_refuse_work {
    leolaniWorld:majority n2mu:refuse leolaniWorld:work .
}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:majority_refuse_work a gaf:Assertion,
            sem:Event ;
        rdfs:label "majority_refuse_work" ;
        gaf:denotedBy leolaniTalk:chat70_utterance4_char0-71 .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat70_utterance4_char0-71 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat70_utterance4_char0-71" ;
        gaf:containsDenotation leolaniWorld:majority,
            leolaniWorld:work ;
        gaf:denotes leolaniWorld:majority_refuse_work ;
        grasp:hasAttribution leolaniTalk:majority_refuse_work_0000 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-07 03:45:12.816617"^^xml1:string ;
        rdf:value "Because the vast majority of Muslims are unemployed and refuse to work."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat70_utterance4 .

    leolaniTalk:majority_refuse_work_0000 a grasp:Attribution ;
        rdfs:label "majority_refuse_work_0000" ;
        grasp:isAttributionFor leolaniTalk:chat70_utterance4_char0-71 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

