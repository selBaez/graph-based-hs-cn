leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:majority a n2mu:majority,
            gaf:Instance ;
        rdfs:label "majority" ;
        gaf:denotedIn leolaniTalk:chat89_utterance5_char0-173 .

    leolaniWorld:persecuted a gaf:Instance ;
        rdfs:label "persecuted" ;
        gaf:denotedIn leolaniTalk:chat89_utterance5_char0-173 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat89 .

    leolaniTalk:chat89 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat89" ;
        n2mu:id "89"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat89_utterance5 .

    leolaniTalk:chat89_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat89_utterance5" ;
        n2mu:id "5"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:majority_been_persecuted a gaf:Assertion,
            sem:Event ;
        rdfs:label "majority_been_persecuted" ;
        gaf:denotedBy leolaniTalk:chat89_utterance5_char0-173 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat89_utterance5_char0-173 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat89_utterance5_char0-173" ;
        gaf:containsDenotation leolaniWorld:majority,
            leolaniWorld:persecuted ;
        gaf:denotes leolaniWorld:majority_been_persecuted ;
        grasp:hasAttribution leolaniTalk:majority_been_persecuted_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 02:52:36.557531"^^xml1:string ;
        rdf:value "How is it possible that the majority of Jews have been formally persecuted over the last several thousand years if they really controlled the world? Weird notion of control."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat89_utterance5 .

    leolaniTalk:majority_been_persecuted_0000 a grasp:Attribution ;
        rdfs:label "majority_been_persecuted_0000" ;
        grasp:isAttributionFor leolaniTalk:chat89_utterance5_char0-173 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:majority_been_persecuted {
    leolaniWorld:majority n2mu:been leolaniWorld:persecuted .
}

