leolaniWorld:Instances {
    leolaniWorld:fine a gaf:Instance ;
        rdfs:label "fine" ;
        gaf:denotedIn leolaniTalk:chat60_utterance4_char0-78 .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:movements a n2mu:movements,
            gaf:Instance ;
        rdfs:label "movements" ;
        gaf:denotedIn leolaniTalk:chat60_utterance4_char0-78 .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat60 .

    leolaniTalk:chat60 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat60" ;
        n2mu:id "60"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat60_utterance4 .

    leolaniTalk:chat60_utterance4 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat60_utterance4" ;
        n2mu:id "4"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:movements_are_fine a gaf:Assertion,
            sem:Event ;
        rdfs:label "movements_are_fine" ;
        gaf:denotedBy leolaniTalk:chat60_utterance4_char0-78 .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat60_utterance4_char0-78 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat60_utterance4_char0-78" ;
        gaf:containsDenotation leolaniWorld:fine,
            leolaniWorld:movements ;
        gaf:denotes leolaniWorld:movements_are_fine ;
        grasp:hasAttribution leolaniTalk:movements_are_fine_0000 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-06 23:14:55.833510"^^xml1:string ;
        rdf:value "Those other movements are fine, feminism instead is a threat to human dignity."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat60_utterance4 .

    leolaniTalk:movements_are_fine_0000 a grasp:Attribution ;
        rdfs:label "movements_are_fine_0000" ;
        grasp:isAttributionFor leolaniTalk:chat60_utterance4_char0-78 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:movements_are_fine {
    leolaniWorld:movements n2mu:are leolaniWorld:fine .
}

