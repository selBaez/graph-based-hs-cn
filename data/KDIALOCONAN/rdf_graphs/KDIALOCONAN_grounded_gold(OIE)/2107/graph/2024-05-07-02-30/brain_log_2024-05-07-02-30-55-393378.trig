        sem:hasBeginTimeStamp "2024-05-07 02:30:44.514475"^^xml1:string ;
        rdf:value "They can love whoever they want in secret. I do not want gays in my children's classes."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat60_utterance2 .

    leolaniTalk:they_want-in_secret_0000 a grasp:Attribution ;
        rdfs:label "they_want-in_secret_0000" ;
        grasp:isAttributionFor leolaniTalk:chat60_utterance2_char0-87 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
