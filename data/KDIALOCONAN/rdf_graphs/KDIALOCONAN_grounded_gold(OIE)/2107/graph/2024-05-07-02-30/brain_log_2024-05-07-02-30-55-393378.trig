leolaniWorld:Instances {
    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:secret a n2mu:secret,
            gaf:Instance ;
        rdfs:label "secret" ;
        gaf:denotedIn leolaniTalk:chat60_utterance2_char0-87 .

    leolaniWorld:they a gaf:Instance ;
        rdfs:label "they" ;
        gaf:denotedIn leolaniTalk:chat60_utterance2_char0-87 .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat60 .

    leolaniTalk:chat60 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat60" ;
        n2mu:id "60"^^xml1:string ;
