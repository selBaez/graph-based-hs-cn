}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:they_want-in_secret a gaf:Assertion,
            sem:Event ;
        rdfs:label "they_want-in_secret" ;
        gaf:denotedBy leolaniTalk:chat60_utterance2_char0-87 .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniWorld:they_want-in_secret {
    leolaniWorld:they n2mu:want_in leolaniWorld:secret .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat60_utterance2_char0-87 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat60_utterance2_char0-87" ;
        gaf:containsDenotation leolaniWorld:secret,
