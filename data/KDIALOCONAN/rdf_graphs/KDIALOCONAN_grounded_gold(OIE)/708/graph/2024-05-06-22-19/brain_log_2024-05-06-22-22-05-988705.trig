leolaniWorld:that_s-new_hs {
    leolaniWorld:that ns1:s_new leolaniWorld:hs .
}

leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:hs a n2mu:agent,
            gaf:Instance ;
        rdfs:label "hs" ;
        gaf:denotedIn leolaniTalk:chat15_utterance5_char0-81 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:that a n2mu:food,
            gaf:Instance ;
        rdfs:label "that" ;
        gaf:denotedIn leolaniTalk:chat15_utterance5_char0-81 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat15 .

    leolaniTalk:chat15 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat15" ;
        n2mu:id "15"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat15_utterance5 .

    leolaniTalk:chat15_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat15_utterance5" ;
        n2mu:id "5"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:that_s-new_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "that_s-new_hs" ;
        gaf:denotedBy leolaniTalk:chat15_utterance5_char0-81 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat15_utterance5_char0-81 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat15_utterance5_char0-81" ;
        gaf:containsDenotation leolaniWorld:hs,
            leolaniWorld:that ;
        gaf:denotes leolaniWorld:that_s-new_hs ;
        grasp:hasAttribution leolaniTalk:that_s-new_hs_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 22:22:01.881368"^^xml1:string ;
        rdf:value "Wait, what do you mean \"most religious organizations\"? That's a new canard to me!"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat15_utterance5 .

    leolaniTalk:that_s-new_hs_0000 a grasp:Attribution ;
        rdfs:label "that_s-new_hs_0000" ;
        grasp:isAttributionFor leolaniTalk:chat15_utterance5_char0-81 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

