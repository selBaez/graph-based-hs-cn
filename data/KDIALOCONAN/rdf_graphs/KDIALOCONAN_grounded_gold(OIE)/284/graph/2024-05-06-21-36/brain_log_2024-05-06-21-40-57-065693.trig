leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:individuals a n2mu:individuals,
            gaf:Instance ;
        rdfs:label "individuals" ;
        gaf:denotedIn leolaniTalk:chat39_utterance5_char0-197 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:myth a n2mu:written-work,
            gaf:Instance ;
        rdfs:label "myth" ;
        gaf:denotedIn leolaniTalk:chat39_utterance5_char0-197 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat39 .

    leolaniTalk:chat39 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat39" ;
        n2mu:id "39"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat39_utterance5 .

    leolaniTalk:chat39_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat39_utterance5" ;
        n2mu:id "5"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:myth_perpetuated-by_individuals {
    leolaniWorld:myth n2mu:perpetuated_by leolaniWorld:individuals .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:myth_perpetuated-by_individuals a gaf:Assertion,
            sem:Event ;
        rdfs:label "myth_perpetuated-by_individuals" ;
        gaf:denotedBy leolaniTalk:chat39_utterance5_char0-197 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat39_utterance5_char0-197 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat39_utterance5_char0-197" ;
        gaf:containsDenotation leolaniWorld:individuals,
            leolaniWorld:myth ;
        gaf:denotes leolaniWorld:myth_perpetuated-by_individuals ;
        grasp:hasAttribution leolaniTalk:myth_perpetuated-by_individuals_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 21:40:47.067446"^^xml1:string ;
        rdf:value "This is a myth perpetuated by individuals who wish to delegitimize and isolate individuals based on their religion. All religions have value and a positive role in society. Judaism is no exception."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat39_utterance5 .

    leolaniTalk:myth_perpetuated-by_individuals_0000 a grasp:Attribution ;
        rdfs:label "myth_perpetuated-by_individuals_0000" ;
        grasp:isAttributionFor leolaniTalk:chat39_utterance5_char0-197 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
