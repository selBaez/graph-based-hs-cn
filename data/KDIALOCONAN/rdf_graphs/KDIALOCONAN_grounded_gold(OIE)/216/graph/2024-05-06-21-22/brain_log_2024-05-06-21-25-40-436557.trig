@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix graspe: <http://groundedannotationframework.org/grasp/emotion#> .
@prefix graspf: <http://groundedannotationframework.org/grasp/factuality#> .
@prefix grasps: <http://groundedannotationframework.org/grasp/sentiment#> .
@prefix leolaniContext: <http://cltl.nl/leolani/context/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:person a n2mu:person,
            gaf:Instance ;
        rdfs:label "person" ;
        gaf:denotedIn leolaniTalk:chat204_utterance3_char0-325 .

    leolaniWorld:twitter a n2mu:twitter,
            gaf:Instance ;
        rdfs:label "twitter" ;
        gaf:denotedIn leolaniTalk:chat204_utterance3_char0-325 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat204 .

    leolaniTalk:chat204 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat204" ;
        n2mu:id "204"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat204_utterance3 .

    leolaniTalk:chat204_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat204_utterance3" ;
        n2mu:id "3"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:person_encounters-beliefs-in_twitter {
    leolaniWorld:person n2mu:encounters_beliefs_in leolaniWorld:twitter .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:person_encounters-beliefs-in_twitter a gaf:Assertion,
            sem:Event ;
        rdfs:label "person_encounters-beliefs-in_twitter" ;
        gaf:denotedBy leolaniTalk:chat204_utterance3_char0-325 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat204_utterance3_char0-325 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat204_utterance3_char0-325" ;
        gaf:containsDenotation leolaniWorld:person,
            leolaniWorld:twitter ;
        gaf:denotes leolaniWorld:person_encounters-beliefs-in_twitter ;
        grasp:hasAttribution leolaniTalk:person_encounters-beliefs-in_twitter_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 21:25:36.368166"^^xml1:string ;
        rdf:value "Animal cruelty isn't right but it also isn't an issue concerning only Muslims. People of all faiths do it. I think it's important to rembember how in twitter a person encounters only beliefs or opinions that coincide with their own. Fake news which put Muslims under a bad light get belived and the selective media feed this."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat204_utterance3 .

    leolaniTalk:person_encounters-beliefs-in_twitter_0000 a grasp:Attribution ;
        rdfs:label "person_encounters-beliefs-in_twitter_0000" ;
        grasp:isAttributionFor leolaniTalk:chat204_utterance3_char0-325 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

