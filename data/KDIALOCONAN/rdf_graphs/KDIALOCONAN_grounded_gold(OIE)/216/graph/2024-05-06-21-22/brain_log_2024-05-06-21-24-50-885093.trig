leolaniWorld:Instances {
    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:it a n2mu:it,
            gaf:Instance ;
        rdfs:label "it" ;
        gaf:denotedIn leolaniTalk:chat204_utterance2_char0-217 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:someone a gaf:Instance ;
        rdfs:label "someone" ;
        gaf:denotedIn leolaniTalk:chat204_utterance2_char0-217 .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat204 .

    leolaniTalk:chat204 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat204" ;
        n2mu:id "204"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat204_utterance2 .

    leolaniTalk:chat204_utterance2 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat204_utterance2" ;
        n2mu:id "2"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:hs a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

leolaniWorld:it_was-sent-by_someone {
    leolaniWorld:it n2mu:was_sent_by leolaniWorld:someone .
}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:it_was-sent-by_someone a gaf:Assertion,
            sem:Event ;
        rdfs:label "it_was-sent-by_someone" ;
        gaf:denotedBy leolaniTalk:chat204_utterance2_char0-217 .

    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat204_utterance2_char0-217 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat204_utterance2_char0-217" ;
        gaf:containsDenotation leolaniWorld:it,
            leolaniWorld:someone ;
        gaf:denotes leolaniWorld:it_was-sent-by_someone ;
        grasp:hasAttribution leolaniTalk:it_was-sent-by_someone_0000 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-06 21:24:44.042359"^^xml1:string ;
        rdf:value "Thank you for the infos, I left the information on the bottom of the original tweet once it was sent to me by someone. Still, they heaps worse. You probably haven't seen the vids where they are killing dogs with acid."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat204_utterance2 .

    leolaniTalk:it_was-sent-by_someone_0000 a grasp:Attribution ;
        rdfs:label "it_was-sent-by_someone_0000" ;
        grasp:isAttributionFor leolaniTalk:chat204_utterance2_char0-217 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
