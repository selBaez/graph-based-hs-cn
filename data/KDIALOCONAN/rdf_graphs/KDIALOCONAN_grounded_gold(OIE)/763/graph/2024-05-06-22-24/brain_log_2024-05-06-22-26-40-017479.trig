leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:country a n2mu:country,
            gaf:Instance ;
        rdfs:label "country" ;
        gaf:denotedIn leolaniTalk:chat215_utterance3_char0-109 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:muslims a n2mu:country,
            gaf:Instance ;
        rdfs:label "muslims" ;
        gaf:denotedIn leolaniTalk:chat215_utterance3_char0-109 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat215 .

    leolaniTalk:chat215 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat215" ;
        n2mu:id "215"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat215_utterance3 .

    leolaniTalk:chat215_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat215_utterance3" ;
        n2mu:id "3"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:muslims_is-in_country a gaf:Assertion,
            sem:Event ;
        rdfs:label "muslims_is-in_country" ;
        gaf:denotedBy leolaniTalk:chat215_utterance3_char0-109 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat215_utterance3_char0-109 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat215_utterance3_char0-109" ;
        gaf:containsDenotation leolaniWorld:country,
            leolaniWorld:muslims ;
        gaf:denotes leolaniWorld:muslims_is-in_country ;
        grasp:hasAttribution leolaniTalk:muslims_is-in_country_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 22:26:35.936520"^^xml1:string ;
        rdf:value "If Islam were a violent religion, there would be riots connected to Muslims in every country where they live."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat215_utterance3 .

    leolaniTalk:muslims_is-in_country_0000 a grasp:Attribution ;
        rdfs:label "muslims_is-in_country_0000" ;
        grasp:isAttributionFor leolaniTalk:chat215_utterance3_char0-109 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:muslims_is-in_country {
    leolaniWorld:muslims n2mu:is_in leolaniWorld:country .
}

