leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:gene,
            n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" ;
        gaf:denotedIn leolaniTalk:chat42_utterance5_char0-85 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:speech a n2mu:university,
            gaf:Instance ;
        rdfs:label "speech" ;
        gaf:denotedIn leolaniTalk:chat42_utterance5_char0-85 .
}

leolaniWorld:Claims {
    leolaniWorld:cn_monitor_speech a gaf:Assertion,
            sem:Event ;
        rdfs:label "cn_monitor_speech" ;
        gaf:denotedBy leolaniTalk:chat42_utterance5_char0-85 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat42_utterance5_char0-85 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat42_utterance5_char0-85" ;
        gaf:containsDenotation leolaniWorld:cn,
            leolaniWorld:speech ;
        gaf:denotes leolaniWorld:cn_monitor_speech ;
        grasp:hasAttribution leolaniTalk:cn_monitor_speech_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 19:59:01.509247"^^xml1:string ;
        rdf:value "Perhaps you should monitor your own hate speech before pointing the finger at others?"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat42_utterance5 .

    leolaniTalk:cn_monitor_speech_0000 a grasp:Attribution ;
        rdfs:label "cn_monitor_speech_0000" ;
        grasp:isAttributionFor leolaniTalk:chat42_utterance5_char0-85 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat42 .

    leolaniTalk:chat42 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat42" ;
        n2mu:id "42"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat42_utterance5 .

    leolaniTalk:chat42_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat42_utterance5" ;
        n2mu:id "5"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:cn_monitor_speech {
    leolaniWorld:cn n2mu:monitor leolaniWorld:speech .
}

