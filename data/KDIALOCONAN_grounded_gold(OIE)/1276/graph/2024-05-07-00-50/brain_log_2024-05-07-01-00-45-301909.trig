}

leolaniWorld:muslims_make-up_important_part {
    leolaniWorld:muslims n2mu:make_up leolaniWorld:important_part .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:muslims_make-up_important_part a gaf:Assertion,
            sem:Event ;
        rdfs:label "muslims_make-up_important_part" ;
        gaf:denotedBy leolaniTalk:chat204_utterance5_char0-88 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat204_utterance5_char0-88 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat204_utterance5_char0-88" ;
        gaf:containsDenotation leolaniWorld:important_part,
            leolaniWorld:muslims ;
        gaf:denotes leolaniWorld:muslims_make-up_important_part ;
        grasp:hasAttribution leolaniTalk:muslims_make-up_important_part_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 01:00:38.472254"^^xml1:string ;
        rdf:value "Muslims make up a very important part of our country. They are our brothers and sisters."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat204_utterance5 .

    leolaniTalk:muslims_make-up_important_part_0000 a grasp:Attribution ;
        rdfs:label "muslims_make-up_important_part_0000" ;
        grasp:isAttributionFor leolaniTalk:chat204_utterance5_char0-88 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
