leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:important_part a gaf:Instance ;
        rdfs:label "important-part" ;
        gaf:denotedIn leolaniTalk:chat204_utterance5_char0-88 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:muslims a n2mu:country,
            gaf:Instance ;
        rdfs:label "muslims" ;
        gaf:denotedIn leolaniTalk:chat204_utterance5_char0-88 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat204 .

    leolaniTalk:chat204 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat204" ;
        n2mu:id "204"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat204_utterance5 .

    leolaniTalk:chat204_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat204_utterance5" ;
        n2mu:id "5"^^xml1:string ;
