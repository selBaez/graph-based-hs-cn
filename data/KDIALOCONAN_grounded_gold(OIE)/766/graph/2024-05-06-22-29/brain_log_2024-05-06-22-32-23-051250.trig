leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:other_words a gaf:Instance ;
        rdfs:label "other-words" ;
        gaf:denotedIn leolaniTalk:chat101_utterance3_char0-180 .

    leolaniWorld:small_part a gaf:Instance ;
        rdfs:label "small-part" ;
        gaf:denotedIn leolaniTalk:chat101_utterance3_char0-180 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat101 .

    leolaniTalk:chat101 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat101" ;
        n2mu:id "101"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat101_utterance3 .

    leolaniTalk:chat101_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat101_utterance3" ;
        n2mu:id "3"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:small_part_is-in_other_words a gaf:Assertion,
            sem:Event ;
        rdfs:label "small_part_is-in_other_words" ;
        gaf:denotedBy leolaniTalk:chat101_utterance3_char0-180 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat101_utterance3_char0-180 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat101_utterance3_char0-180" ;
        gaf:containsDenotation leolaniWorld:other_words,
            leolaniWorld:small_part ;
        gaf:denotes leolaniWorld:small_part_is-in_other_words ;
        grasp:hasAttribution leolaniTalk:small_part_is-in_other_words_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-06 22:32:09.460097"^^xml1:string ;
        rdf:value "In Europe, Muslims make up around 5% of the population. In other words, Muslims are a very small but important part of our society. Moreover, ethnicity does not relate to religion."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat101_utterance3 .

    leolaniTalk:small_part_is-in_other_words_0000 a grasp:Attribution ;
        rdfs:label "small_part_is-in_other_words_0000" ;
        grasp:isAttributionFor leolaniTalk:chat101_utterance3_char0-180 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:small_part_is-in_other_words {
