}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat65 .

    leolaniTalk:chat65 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat65" ;
        n2mu:id "65"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat65_utterance5 .

    leolaniTalk:chat65_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat65_utterance5" ;
        n2mu:id "5"^^xml1:string ;
