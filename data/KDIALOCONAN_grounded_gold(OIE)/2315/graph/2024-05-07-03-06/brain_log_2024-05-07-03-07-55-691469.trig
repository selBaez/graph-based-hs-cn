    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:hs_are_humans a gaf:Assertion,
            sem:Event ;
        rdfs:label "hs_are_humans" ;
        gaf:denotedBy leolaniTalk:chat65_utterance5_char0-113 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat65_utterance5_char0-113 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat65_utterance5_char0-113" ;
        gaf:containsDenotation leolaniWorld:hs,
            leolaniWorld:humans ;
        gaf:denotes leolaniWorld:hs_are_humans ;
        grasp:hasAttribution leolaniTalk:hs_are_humans_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 03:07:50.652624"^^xml1:string ;
        rdf:value "They are. If there is a problem, a solution can be found through dialogue instead of division. We are all humans."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat65_utterance5 .

    leolaniTalk:hs_are_humans_0000 a grasp:Attribution ;
        rdfs:label "hs_are_humans_0000" ;
        grasp:isAttributionFor leolaniTalk:chat65_utterance5_char0-113 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:hs_are_humans {
