leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:hs a n2mu:agent,
            gaf:Instance ;
        rdfs:label "hs" ;
        gaf:denotedIn leolaniTalk:chat65_utterance5_char0-113 .

    leolaniWorld:humans a n2mu:humans,
            gaf:Instance ;
        rdfs:label "humans" ;
        gaf:denotedIn leolaniTalk:chat65_utterance5_char0-113 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .
}

leolaniWorld:leolani_know_cn {
