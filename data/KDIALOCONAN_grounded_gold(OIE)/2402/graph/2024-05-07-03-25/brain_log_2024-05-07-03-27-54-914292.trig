leolaniWorld:Instances {
    leolaniWorld:hs a n2mu:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:immigrants a n2mu:immigrants,
            gaf:Instance ;
        rdfs:label "immigrants" ;
        gaf:denotedIn leolaniTalk:chat80_utterance2_char0-95 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:live_here a gaf:Instance ;
        rdfs:label "live-here" ;
        gaf:denotedIn leolaniTalk:chat80_utterance2_char0-95 .
}

leolaniWorld:immigrants_were_live_here {
