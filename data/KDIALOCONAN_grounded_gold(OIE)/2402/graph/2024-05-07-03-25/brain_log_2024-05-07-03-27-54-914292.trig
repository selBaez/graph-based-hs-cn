}

leolaniWorld:leolani_know_hs {
    leolaniWorld:leolani n2mu:know leolaniWorld:hs .
}

leolaniWorld:Claims {
    leolaniWorld:immigrants_were_live_here a gaf:Assertion,
            sem:Event ;
        rdfs:label "immigrants_were_live_here" ;
        gaf:denotedBy leolaniTalk:chat80_utterance2_char0-95 .

    leolaniWorld:leolani_know_hs a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_hs" .

    leolaniWorld:hs owl:sameAs leolaniWorld:hs .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat80_utterance2_char0-95 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat80_utterance2_char0-95" ;
        gaf:containsDenotation leolaniWorld:immigrants,
            leolaniWorld:live_here ;
        gaf:denotes leolaniWorld:immigrants_were_live_here ;
        grasp:hasAttribution leolaniTalk:immigrants_were_live_here_0000 ;
        grasp:wasAttributedTo leolaniWorld:hs ;
        sem:hasBeginTimeStamp "2024-05-07 03:27:47.985417"^^xml1:string ;
        rdf:value "The problem isn't only housing: if immigrants were to live here, it would be hard to find a job"^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat80_utterance2 .

    leolaniTalk:immigrants_were_live_here_0000 a grasp:Attribution ;
        rdfs:label "immigrants_were_live_here_0000" ;
        grasp:isAttributionFor leolaniTalk:chat80_utterance2_char0-95 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
