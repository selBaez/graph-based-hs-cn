}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat80 .

    leolaniTalk:chat80 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat80" ;
        n2mu:id "80"^^xml1:string ;
