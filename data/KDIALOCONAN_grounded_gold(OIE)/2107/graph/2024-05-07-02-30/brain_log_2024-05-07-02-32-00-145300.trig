@prefix gaf: <http://groundedannotationframework.org/gaf#> .
@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix graspe: <http://groundedannotationframework.org/grasp/emotion#> .
@prefix graspf: <http://groundedannotationframework.org/grasp/factuality#> .
@prefix grasps: <http://groundedannotationframework.org/grasp/sentiment#> .
@prefix leolaniContext: <http://cltl.nl/leolani/context/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:life_choices a gaf:Instance ;
        rdfs:label "life-choices" ;
        gaf:denotedIn leolaniTalk:chat60_utterance5_char0-93 .

    leolaniWorld:right a n2mu:right,
            gaf:Instance ;
        rdfs:label "right" ;
        gaf:denotedIn leolaniTalk:chat60_utterance5_char0-93 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat60 .

    leolaniTalk:chat60 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat60" ;
        n2mu:id "60"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat60_utterance5 .

    leolaniTalk:chat60_utterance5 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat60_utterance5" ;
        n2mu:id "5"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:right_make_life_choices a gaf:Assertion,
            sem:Event ;
        rdfs:label "right_make_life_choices" ;
        gaf:denotedBy leolaniTalk:chat60_utterance5_char0-93 .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat60_utterance5_char0-93 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat60_utterance5_char0-93" ;
        gaf:containsDenotation leolaniWorld:life_choices,
            leolaniWorld:right ;
        gaf:denotes leolaniWorld:right_make_life_choices ;
        grasp:hasAttribution leolaniTalk:right_make_life_choices_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 02:31:53.222062"^^xml1:string ;
        rdf:value "Anyone has the right to make personal life choices without the government having a say in it."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat60_utterance5 .

    leolaniTalk:right_make_life_choices_0000 a grasp:Attribution ;
        rdfs:label "right_make_life_choices_0000" ;
        grasp:isAttributionFor leolaniTalk:chat60_utterance5_char0-93 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:right_make_life_choices {
    leolaniWorld:right n2mu:make leolaniWorld:life_choices .
}

