@prefix gaf: <http://groundedannotationframework.org/gaf#> .
@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix graspe: <http://groundedannotationframework.org/grasp/emotion#> .
@prefix graspf: <http://groundedannotationframework.org/grasp/factuality#> .
@prefix grasps: <http://groundedannotationframework.org/grasp/sentiment#> .
@prefix leolaniContext: <http://cltl.nl/leolani/context/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniWorld:Instances {
    leolaniWorld:cn a n2mu:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:hs a n2mu:agent,
            gaf:Instance ;
        rdfs:label "hs" ;
        gaf:denotedIn leolaniTalk:chat73_utterance1_char0-125 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:problem a n2mu:problem,
            gaf:Instance ;
        rdfs:label "problem" ;
        gaf:denotedIn leolaniTalk:chat73_utterance1_char0-125 .
}

leolaniWorld:leolani_know_cn {
    leolaniWorld:leolani n2mu:know leolaniWorld:cn .
}

leolaniTalk:Interactions {
    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat73 .

    leolaniTalk:chat73 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat73" ;
        n2mu:id "73"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat73_utterance1 .

    leolaniTalk:chat73_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat73_utterance1" ;
        n2mu:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniWorld:cn a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .
}

leolaniWorld:Claims {
    leolaniWorld:hs_target_problem a gaf:Assertion,
            sem:Event ;
        rdfs:label "hs_target_problem" ;
        gaf:denotedBy leolaniTalk:chat73_utterance1_char0-125 .

    leolaniWorld:leolani_know_cn a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_cn" .

    leolaniWorld:cn owl:sameAs leolaniWorld:cn .
}

leolaniWorld:hs_target_problem {
    leolaniWorld:hs n2mu:target leolaniWorld:problem .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat73_utterance1_char0-125 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat73_utterance1_char0-125" ;
        gaf:containsDenotation leolaniWorld:hs,
            leolaniWorld:problem ;
        gaf:denotes leolaniWorld:hs_target_problem ;
        grasp:hasAttribution leolaniTalk:hs_target_problem_0000 ;
        grasp:wasAttributedTo leolaniWorld:cn ;
        sem:hasBeginTimeStamp "2024-05-07 00:07:25.367899"^^xml1:string ;
        rdf:value "I agree that we need to tackle this serious breach of human rights but scapegoating isn't helping us target the real problem."^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat73_utterance1 .

    leolaniTalk:hs_target_problem_0000 a grasp:Attribution ;
        rdfs:label "hs_target_problem_0000" ;
        grasp:isAttributionFor leolaniTalk:chat73_utterance1_char0-125 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

