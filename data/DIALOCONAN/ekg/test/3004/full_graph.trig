        rdfs:label "chat3004_utterance6" ;
        hs:id "6"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:MX a hs:country,
            hs:location,
            sem:Place ;
        rdfs:label "MX" .

    leolaniWorld:Mexico_City a hs:city,
            hs:location,
            hs:region,
            sem:Place ;
        rdfs:label "Mexico City" .

    leolaniWorld:cn a hs:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "cn" .

    leolaniWorld:hs a hs:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "hs" .
}

