    leolaniTalk:chat3004_utterance4 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat3004_utterance4" ;
        hs:id "4"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniTalk:chat3004_utterance6 a grasp:Utterance,
