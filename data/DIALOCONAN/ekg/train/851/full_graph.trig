                            owl:onProperty grasp:isAttributionFor ;
                            owl:someValuesFrom gaf:Mention ] [ a owl:Restriction ;
                            owl:onProperty rdf:value ;
                            owl:someValuesFrom grasp:AttributionValue ] ) ] .

