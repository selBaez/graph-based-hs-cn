        hs:id "158"^^xml1:string ;
        hs:in leolaniWorld:MX,
            leolaniWorld:Mexico_City .

    leolaniTalk:chat2490 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat2490" ;
        hs:id "2490"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat2490_utterance1,
            leolaniTalk:chat2490_utterance2,
            leolaniTalk:chat2490_utterance3,
            leolaniTalk:chat2490_utterance4 .

    leolaniTalk:chat2490_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat2490_utterance1" ;
        hs:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniTalk:chat2490_utterance2 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat2490_utterance2" ;
        hs:id "2"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniTalk:chat2490_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat2490_utterance3" ;
        hs:id "3"^^xml1:string ;
        sem:hasActor leolaniWorld:cn .

    leolaniTalk:chat2490_utterance4 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat2490_utterance4" ;
        hs:id "4"^^xml1:string ;
        sem:hasActor leolaniWorld:hs .

    leolaniWorld:MX a hs:country,
            hs:location,
