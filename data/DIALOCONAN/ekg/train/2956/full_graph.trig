            leolaniTalk:chat2956_utterance3_char0-241,
            leolaniTalk:chat2956_utterance4_char0-52 .

    leolaniWorld:Nazis a gaf:Instance ;
        rdfs:label "nazis" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance0_char0-93 .

    leolaniWorld:They a gaf:Instance ;
        rdfs:label "they" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance1_char0-114 .

    leolaniWorld:cn a hs:person,
            gaf:Instance ;
        rdfs:label "cn" .

    leolaniWorld:completely_unacceptable a gaf:Instance ;
        rdfs:label "completely-unacceptable" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance3_char0-241 .

    leolaniWorld:get a gaf:Instance ;
        rdfs:label "get" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance2_char0-137 .

    leolaniWorld:hs a hs:person,
            gaf:Instance ;
        rdfs:label "hs" .

    leolaniWorld:innocent_people_as_possible a gaf:Instance ;
