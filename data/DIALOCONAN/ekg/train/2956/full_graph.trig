    leolaniWorld:people a gaf:Instance ;
        rdfs:label "people" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance3_char0-241 .

    leolaniWorld:race a gaf:Instance ;
        rdfs:label "race" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance0_char0-93 .

    leolaniWorld:rid a gaf:Instance ;
        rdfs:label "rid" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance2_char0-137 .

    leolaniWorld:rid_of_Jews a gaf:Instance ;
        rdfs:label "rid-of-jews" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance2_char0-137 .

    leolaniWorld:right a gaf:Instance ;
        rdfs:label "right" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance0_char0-93,
            leolaniTalk:chat2956_utterance1_char0-114 .

    leolaniWorld:they a gaf:Instance ;
        rdfs:label "they" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance1_char0-114,
            leolaniTalk:chat2956_utterance2_char0-137 .

    leolaniWorld:to_get a gaf:Instance ;
        rdfs:label "to-get" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance2_char0-137 .

    leolaniWorld:unacceptable a gaf:Instance ;
        rdfs:label "unacceptable" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance3_char0-241 .

    leolaniWorld:you a gaf:Instance ;
        rdfs:label "you" ;
        gaf:denotedIn leolaniTalk:chat2956_utterance3_char0-241 .
}

