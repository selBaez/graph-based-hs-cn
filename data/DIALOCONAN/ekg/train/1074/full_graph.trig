            owl:NamedIndividual .

    graspe:disgust a grasp:EmotionValue,
            owl:NamedIndividual .

    graspe:fear a grasp:EmotionValue,
            owl:NamedIndividual .

    graspe:happiness a grasp:EmotionValue,
            owl:NamedIndividual .

    graspe:sadness a grasp:EmotionValue,
