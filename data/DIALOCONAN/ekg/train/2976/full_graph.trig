    eps:hasContext a owl:ObjectProperty ;
        rdfs:domain gaf:Instance,
            sem:Event ;
        rdfs:range eps:Context ;
        rdfs:subPropertyOf eps:contextProperty .

    eps:hasDetection a owl:ObjectProperty ;
        rdfs:domain eps:Context ;
