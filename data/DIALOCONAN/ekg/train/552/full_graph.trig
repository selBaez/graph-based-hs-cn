    grasp:SentimentValue a owl:Class ;
        rdfs:label "Sentiment Value" ;
        rdfs:comment "The sentiment expressed by a gaf:Mention"^^xsd:string ;
        rdfs:subClassOf grasp:AttributionValue ;
        owl:equivalentClass [ a owl:Class ;
