    grasp:FactualityValue a owl:Class ;
        rdfs:label "Factuality value"^^xsd:string ;
        rdfs:subClassOf grasp:AttributionValue ;
        owl:disjointWith grasp:SentimentValue .

    grasp:SentimentValue a owl:Class ;
