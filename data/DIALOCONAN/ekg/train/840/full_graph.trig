                            owl:onProperty rdf:value ;
                            owl:someValuesFrom grasp:AttributionValue ] ) ] .

    grasp:Detection a owl:Class ;
        rdfs:subClassOf sem:Event .

    grasp:isAttributionFor a owl:ObjectProperty ;
        rdfs:domain grasp:Attribution ;
